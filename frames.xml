<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

	<Frame name="SettingsColorControlTemplate" inherits="SettingsListElementTemplate" mixin="SettingsColorControlMixin" virtual="true">
		<Size x="280" y="26"/>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>



 	 <Button name="AddonCompartmentButtonsButtonFrame" mixin="AddonCompartmentButtonsButtonMixin" virtual="true">
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="Icon" setAllPoints="true"/>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnClick method="OnClick"/>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
		</Scripts>
		<HighlightTexture alphaMode="ADD" setAllPoints="true" alpha="0.1">
		    <Color r="1" g="1" b="1" a="1" />
		</HighlightTexture>
	</Button>


	<Frame name="AddonCompartmentButtonsFrame" hidden="true"  mixin="AddonCompartmentButtonsMixin" >
		<Size x="0" y="0"/>
		<Anchors>
			<Anchor point="TOPRIGHT"/>
		</Anchors>
		<Frames>
			<Frame parentKey="Contents"  inherits="TooltipBackdropTemplate" enableMouse="true" >
				<Anchors>
					<Anchor point="TOPLEFT" x="-4" y="4"/>
					<Anchor point="BOTTOMRIGHT" x="4" y="-4"/>
				</Anchors>
			</Frame>
		 	<Button parentKey="Drag" mixin="AddonCompartmentButtonsDragMixin" >
				<Size x="10" y="20"/>
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPRIGHT" x="4" y="0"/>
					<Anchor point="BOTTOMLEFT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" x="4" y="0"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture parentKey="Icon" setAllPoints="true">
							<Color r=".1" g=".1" b=".1" a="1" />
						</Texture>
					</Layer>
				</Layers>
				<Scripts>
					<OnClick method="OnClick"/>
				</Scripts>
				<NormalTexture parentKey="NormalTexture" atlas="auctionhouse-rowstripe-1" setAllPoints="true"/>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>


</Ui>